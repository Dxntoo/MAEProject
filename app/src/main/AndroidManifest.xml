<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.etrite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dantoo">
        <activity android:name="com.example.etrite.UserProfileActivity"></activity>
        <activity android:name="com.example.etrite.MainActivity" />
        <activity android:name="com.example.etrite.ForgotPasswordActivity" />
        <activity android:name="com.example.etrite.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.etrite.RegisterActivity" />
        <activity
            android:name="com.example.etrite.Dashboard"
            android:label="@string/title_activity_dashboard" />
        <activity android:name="com.example.etrite.LoginActivity" />
    </application>

</manifest>